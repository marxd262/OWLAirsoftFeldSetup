                <RadzenDropDown TValue="GameMode" ValueChanged="GameModeChanged" Data="Enum.GetValues(typeof(GameMode))" Value="GameMode.Timer"/>
                <RadzenButton ButtonStyle="@(GetButtonStyle)" Text="@(GetButtonText)" Click="ButtonClick"/>
        </RadzenStack>
</RadzenCard>